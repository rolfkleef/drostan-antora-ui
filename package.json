{
  "name": "drostan-antora-ui",
  "description": "Drostan's version of a UI for the Antora documentation site generator.",
  "version": "0.1.6",
  "homepage": "https://github.com/rolfkleef/drostan-antora-ui",
  "license": "MPL-2.0",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rolfkleef/drostan-antora-ui.git"
  },
  "scripts": {
    "dev": "vite dev",
    "preview": "vite preview --outDir build",
    "docs": "antora --stacktrace antora/playbooks/preview.yml",
    "bundle": "vite build",
    "create-release": "commit-and-tag-version",
    "update-release": "commit-and-tag-version --skip.bump --tag-force"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "antora": "^3.1.7",
    "commit-and-tag-version": "^12.4.1",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-zip-pack": "^1.2.3"
  },
  "dependencies": {
    "@fontsource/alegreya-sans": "^5.0.13",
    "@fontsource/material-icons": "^4.2.4",
    "@fontsource/roboto": "~4.5",
    "@fontsource/roboto-mono": "~4.5"
  }
}
